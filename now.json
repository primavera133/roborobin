{
  "version": 2,
  "name": "roborobin",
  "alias": [
    "beta.roborobin.net"
  ],
  "builds": [
    {
      "src": "/index.html",
      "use": "@now/static"
    },
    {
      "src": "/static/**/*.*",
      "use": "@now/static"
    },
    {
      "src": "/img/**/*.*",
      "use": "@now/static"
    },
    {
      "src": "/api/recordings/index.js",
      "use": "@now/node"
    },
    {
      "src": "/api/translate/index.js",
      "use": "@now/node"
    }
  ]
}